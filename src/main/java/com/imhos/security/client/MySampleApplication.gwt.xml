<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.0//EN"
        "http://google-web-toolkit.googlecode.com/svn/releases/2.0/distro-source/core/src/gwt-module.dtd">
<module rename-to="application">

    <!-- Inherit the core Web Toolkit stuff.                  -->
    <inherits name='com.google.gwt.json.JSON'/>
    <inherits name='com.google.gwt.user.User'/>
    <inherits name='com.imhos.security.shared.SharedSecurity'/>

    <!-- Specify the app entry point class.                   -->
    <entry-point class='com.imhos.security.client.module.SampleApplication'/>


    <!-- Specify the app servlets.                   -->
    <!--<servlet path='/SampleApplicationService' class='third.service.gwt.SampleApplicationServiceImpl>-->
    <source path='module' />

</module>